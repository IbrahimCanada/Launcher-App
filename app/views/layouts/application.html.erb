<!DOCTYPE html>
<html>
	<head>
	  <title>Welcome to LaunchBox!</title>
	  <%= stylesheet_link_tag :all %>
	  <%= javascript_include_tag :defaults %>
	  <%= csrf_meta_tag %>
	</head>
	<body>
		<%= render 'layouts/header' %>
		<% flash.each do |key, value| %>			
			<% unless key.to_s == "user_show" %>		
	    	<div class="flash <%=key %>"><%= value %></div>
	    <% end %>
	  <% end %>
		
		<%= yield %>

	</body>
</html>
